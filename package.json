{
	"name": "keys-translations-manager",
	"version": "1.3.1",
	"private": true,
	"description": "A web application for locale's keys and translations management.",
	"keywords": [
		"i18n",
		"internationalization",
		"lang",
		"language",
		"l10n",
		"locale",
		"localization",
		"multilingual",
		"translation",
		"MERN"
	],
	"author": "chejen",
	"scripts": {
		"postinstall": "npm run build",
		"lint": "eslint \"src/**/*.js\"",
		"dev": "cross-env NODE_ENV=development nodemon --exec babel-node -- ./Server.js",
		"start": "cross-env NODE_ENV=production babel-node ./Server.js",
		"build": "npm run build:css && npm run build:js",
		"build:css": "lessc less/app.less public/css/app.css && cleancss public/css/app.css -o public/css/app.css",
		"build:js": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
		"test": "cross-env NODE_ENV=test mocha -R spec --compilers js:babel-core/register  --require ./tests/testHelper.js tests/**/*.js",
		"test:watch": "npm test -- --watch",
		"coverage": "cross-env NODE_ENV=test babel-node node_modules/.bin/babel-istanbul cover node_modules/mocha/bin/_mocha tests/**/*.js --report lcovonly -- -R spec",
		"codecov": "cat ./coverage/lcov.info | codecov"
	},
	"pre-commit": [
		"lint",
		"test"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/chejen/keys-translations-manager"
	},
	"engines": {
		"node": ">=5"
	},
	"dependencies": {
		"archiver": "^1.3.0",
		"body-parser": "^1.15.2",
		"bootstrap": "~3.3.7",
		"d3-hierarchy": "^1.1.4",
		"d3-selection": "^1.0.6",
		"d3-zoom": "^1.1.4",
		"es6-promise": "^4.0.5",
		"express": "^4.14.0",
		"isomorphic-fetch": "^2.2.1",
		"keys-translations-manager-core": "^1.3.0",
		"mongoose": "^4.8.5",
		"multiparty": "^4.1.2",
		"react": "~15.4.2",
		"react-addons-pure-render-mixin": "~15.4.2",
		"react-bootstrap": "~0.31.0",
		"react-bootstrap-table": "~3.3.2",
		"react-dom": "~15.4.2",
		"react-dropzone": "^3.8.0",
		"react-redux": "^5.0.1",
		"react-router": "^4.0.0",
		"react-router-dom": "^4.0.0",
		"redux": "^3.6.0",
		"redux-thunk": "^2.1.0",
		"socket.io": "^1.7.2"
	},
	"devDependencies": {
		"babel-cli": "^6.18.0",
		"babel-core": "^6.21.0",
		"babel-eslint": "^7.1.1",
		"babel-istanbul": "~0.12.1",
		"babel-loader": "^7.0.0",
		"babel-plugin-add-module-exports": "~0.2.1",
		"babel-preset-es2015": "^6.18.0",
		"babel-preset-react": "^6.16.0",
		"babel-preset-react-hmre": "^1.1.1",
		"babel-preset-stage-0": "^6.16.0",
		"chai": "^3.5.0",
		"clean-css-cli": "^4.0.8",
		"codecov": "^2.1.0",
		"compression": "^1.6.2",
		"cross-env": "^4.0.0",
		"css-loader": "^0.28.0",
		"ejs": "^2.5.5",
		"enzyme": "^2.7.0",
		"eslint": "^3.13.0",
		"eslint-loader": "^1.6.1",
		"eslint-plugin-babel": "^4.0.0",
		"eslint-plugin-i18n": "^1.1.0",
		"eslint-plugin-react": "^6.9.0",
		"eventsource-polyfill": "~0.9.6",
		"istanbul": "~0.4.5",
		"jsdom": "^9.9.1",
		"less": "^2.7.2",
		"less-loader": "^4.0.2",
		"mocha": "^3.2.0",
		"nock": "^9.0.2",
		"nodemon": "^1.11.0",
		"pre-commit": "^1.2.2",
		"react-addons-test-utils": "~15.4.2",
		"redux-mock-store": "^1.2.1",
		"rename": "^1.0.4",
		"serve-favicon": "^2.3.2",
		"sinon": "^2.0.0",
		"sinon-chai": "^2.8.0",
		"style-loader": "~0.17.0",
		"webpack": "^2.2.1",
		"webpack-dev-middleware": "^1.10.1",
		"webpack-hot-middleware": "^2.17.1",
		"webpack-strip": "~0.1.0"
	},
	"license": "MIT"
}
